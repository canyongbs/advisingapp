        "barryvdh/laravel-ide-helper": "^3.5",
        "brianium/paratest": "^7.2",
        "doctrine/dbal": "^4.2.3",
        "friendsofphp/php-cs-fixer": "^3.60",
        "larastan/larastan": "^3.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-faker": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "pestphp/pest-plugin-livewire": "^3.0",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11.0",
        "rector/rector": "^2.0",
        "spatie/laravel-ray": "^1.40.2",
        "spatie/pest-plugin-test-time": "^2.1",
        "worksome/request-factories": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Migrations\\Concerns\\": "database/migrations/Concerns/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "exclude-from-classmap": [
            "vendor/laravel/octane/src/RoadRunner/RoadRunnerClient.php",
            "vendor/openai-php/client/src/Responses/Threads/Runs/ThreadRunResponse.php"
        ],
        "files": [
            "overrides/OpenAI/Responses/Threads/Runs/ThreadRunResponse.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "refresh-database": [
            "@php artisan tenants:artisan \"migrate:fresh --database=tenant\"",
            "@php artisan tenants:artisan \"db:seed --database=tenant\""
        ],
        "php-cs-format": [
            "export PHP_CS_FIXER_IGNORE_ENV=1 ; ./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix -v --config=./php-cs-fixer.php"
        ],
        "php-cs-format-dryrun": [
            "export PHP_CS_FIXER_IGNORE_ENV=1 ; @php-cs-format --dry-run --stop-on-violation"
        ],
        "php-cs-clear-cache": [
            "rm -f ./.php-cs-fixer.cache"
        ],
        "prettier": [
            "./node_modules/.bin/prettier \"resources/**/*{.css,.blade.php,.js}\" \"app-modules/**/resources/**/*{.css,.blade.php,.js}\" \"graphql/*.graphql\" \"app-modules/**/graphql/*.graphql\" \"portals/**/*{.vue,.js}\" \"widgets/**/*{.vue,.js}\" \"resources/views/vendor/**/*{.css,.blade.php,.js}\" --config ./.prettierrc.json --ignore-path ./.prettierignore --cache"
        ],
        "prettier-format": [
            "@prettier --write"
        ],
        "prettier-format-dryrun": [
            "@prettier --check"
        ],
        "clear-prettier-cache": [
            "rm -f ./node_modules/.cache/prettier/.prettier-cache"
        ],
        "format-dryrun": [
            "@php-cs-format-dryrun",
            "@prettier-format-dryrun"
        ],
        "format": [
            "@php-cs-format",
            "@prettier-format"
        ],
        "generate-helper-files": [
            "@php artisan app:generate-helper-files",
            "./vendor/bin/rector",
            "@php-cs-format"
        ],
        "lint": [
            "./vendor/bin/phpstan analyse --memory-limit=2G"
        ],
        "checks": [
            "@format-dryrun",
            "@lint"
        ],
        "ci": [
            "rm -rf ./vendor",
            "composer install"
        ],
        "generate-api-docs": [
            "./node_modules/.bin/prettier \"graphql/*.graphql\" \"app-modules/**/graphql/*.graphql\" --config ./.prettierrc.json --ignore-path ./.prettierignore --cache --write",
            "@php artisan api:print-schema",
            "npm run api-docs:generate"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        },
        "laravel": {
            "dont-discover": [
                "nesbot/carbon",
                "spatie/laravel-multitenancy"
            ]
        },
        "google/apiclient-services": [
            "Oauth2",
            "Calendar"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "cache-dir": ".composer/cache",
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        },
        "audit": {
            "abandoned": "report"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "path",
            "url": "app-modules/*",
            "options": {
                "symlink": true
            }
        }
    ]
}
