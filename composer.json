{
    "name": "canyongbs/assistforhighered-2",
    "type": "project",
    "description": "ASSIST stands for Academic & Student Success Integrated Support Technology. This code represents the ASSIST 2.0 CRM, a product within the ASSIST For Higher Ed platform offering.",
    "keywords": ["higher", "education", "academic", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^2.11",
        "spatie/laravel-medialibrary": "^10.7"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.21",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.6",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0",
        "spatie/laravel-ray": "^1.32"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "php-cs-lint": [
            "./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix -v --config=./php-cs-fixer.php"
        ],
        "php-cs-lint-dryrun": [
            "@php-cs-lint --dry-run --stop-on-violation"
        ],
        "php-cs-clear-cache": [
            "rm -f ./.php-cs-fixer.cache"
        ],
        "prettier": [
            "./node_modules/.bin/prettier \"resources/**/*.css\" \"resources/**/*.blade.php\" \"resources/**/*.js\" --config ./.prettierrc.json --ignore-path ./.prettierignore --cache"
        ],
        "prettier-lint": [
            "@prettier --write"
        ],
        "prettier-lint-dryrun": [
            "@prettier --check"
        ],
        "clear-prettier-cache": [
            "rm -f ./node_modules/.cache/prettier/.prettier-cache"
        ],
        "lint-dryrun": [
            "@php-cs-lint-dryrun",
            "@prettier-lint-dryrun"
        ],
        "lint": [
            "@php-cs-lint",
            "@prettier-lint"
        ],
        "larastan": [
            "./vendor/bin/phpstan analyse --memory-limit=2G"
        ],
        "checks": [
            "@lint-dryrun",
            "@larastan"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "prefer-stable": true
}
